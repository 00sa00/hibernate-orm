<?xml version="1.0"?>
<hibernate-mapping package="org.hibernate.test.collection.set.element" xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
                   xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping hibernate-mapping-4.0.xsd"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<class name="User" table="`Users`">
		<id name="userName"/>
		<set name="sessionAttributeNames" lazy="true">
			<key column="userName"/>
			<element column="`attributeName`" type="string"/>
		</set>
	</class>

</hibernate-mapping>
